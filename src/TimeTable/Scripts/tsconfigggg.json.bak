{
  "compileOnSave": false,
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../wwwroot/app/",
    "removeComments": false,
    "sourceRoot": "../wwwroot/app/",
    "sourceMap": true,
    "target": "es5",
    "watch": false
  },
  "exclude": [
    "node_modules",
    "*.js"
  ]
}
